#include <bits/stdc++.h>
using namespace std;

int main(){
    vector<string> art = {
        "  __   _      __   _   ",
        " / ___| | |    / ___| / _ \\  ",
        "| |     | |   | |    | | | | ",
        "| |___  | |___| |___ | |_| | ",
        " \\____| |_____\\____| \\___/  ",
        "                             ",
        "   ___  ++   ++  Logo in C++"
    };
    for(auto &s: art) cout << s << "\n";
    return 0;
}
